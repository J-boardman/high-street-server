<!DOCTYPE class_bookings [<!ELEMENT class_bookings (class_details, bookings)>
<!ELEMENT class_details (class_name, trainer, day, start_time, end_time)>
<!ELEMENT class_name (#PCDATA)>
<!ELEMENT trainer (#PCDATA)>
<!ELEMENT day (#PCDATA)>
<!ELEMENT start_time (#PCDATA)>
<!ELEMENT end_time (#PCDATA)>
<!ELEMENT bookings (booking*)>
<!ELEMENT booking (id, customer)>
<!ELEMENT id (#PCDATA)>
<!ELEMENT customer (#PCDATA)>
]>
<class_bookings>
  <class_details>
    <class_name><%= bookings[0].class_name %></class_name>
    <trainer><%= bookings[0].trainer %></trainer>
    <day><%= bookings[0].day %></day>
    <start_time><%= bookings[0].start_time.slice(0,5) %></start_time>
    <end_time><%= bookings[0].end_time.slice(0,5) %></end_time>
  </class_details>
  <bookings>
    <% for(let booking of bookings) { %>
    <booking>
      <id><%= booking.booking_id %></id>
      <customer><%= booking.customer %></customer>
    </booking>
    <% } %>
  </bookings>
</class_bookings>