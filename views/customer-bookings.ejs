<!DOCTYPE bookings [<!ELEMENT bookings (booking*)>
  <!ELEMENT booking (class_name, trainer, day, start_time, end_time)>
  <!ELEMENT class_name (#PCDATA)>
  <!ELEMENT trainer (#PCDATA)>
  <!ELEMENT day (#PCDATA)>
  <!ELEMENT start_time (#PCDATA)>
  <!ELEMENT end_time (#PCDATA)>
  ]>
  <bookings>
    <% for(let booking of bookings) { %>
    <booking>
      <class_name><%= booking.class_name %></class_name>
      <trainer><%= booking.trainer %></trainer>
      <day><%= booking.day %></day>
      <start_time><%= booking.start_time.slice(0,5) %> -</start_time>
      <end_time><%= booking.end_time.slice(0,5) %></end_time>
    </booking>
  <% } %>
</bookings>